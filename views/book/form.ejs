<div class="col-lg-4">
  <h1>Add Book</h1>
  <form ng-submit="addBook()" class="form-horizontal">
    <div class="form-group">
      <label for="title" class="col-lg-3 control-label">Title</label>
      <div class="col-lg-9"><input type="text" ng-model="title" name="title" id="title" class="form-control"></div>
    </div>

    <div class="form-group">
      <label for="author" class="col-lg-3 control-label">Author</label>
      <div class="col-lg-9"><input type="text" ng-model="author" name="author" id="author" class="form-control"></div>
    </div>

    <%- partial('rating.ejs') %>
  </form>
</div>
